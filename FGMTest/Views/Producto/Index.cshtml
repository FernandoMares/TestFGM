@model IEnumerable<FGMTest.Models.PRODUCTO>

@{
    ViewBag.Title = "Index";
}

<h2>¡Listado productos!</h2>


<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            Nombre
        </th>
        <th>
            Descripción
        </th>
        <th>
            Precio
        </th>
        <th>
            Producto Eliminado
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PROD_ID)
        </td>
        <td>
            @item.PROD_NOMBRE
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROD_DESCRIPCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROD_PRECIO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROD_ELIMINADO)
        </td>
        <td>

        </td>
    </tr>
}

</table>
